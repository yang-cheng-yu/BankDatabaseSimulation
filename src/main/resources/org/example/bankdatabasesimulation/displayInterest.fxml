<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.bankdatabasesimulation.DisplayInterestController">
   <children>
      <Label fx:id="titleLabel" layoutX="216.0" layoutY="25.0" text="Interest Rate">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="interestRateTable" layoutX="14.0" layoutY="70.0" prefHeight="313.0" prefWidth="151.0">
        <columns>
          <TableColumn fx:id="yearTable" prefWidth="75.0" text="Year" />
          <TableColumn fx:id="moneytable" prefWidth="75.0" text="Money" />
        </columns>
      </TableView>
      <Label fx:id="interestRateTextLabel" layoutX="192.0" layoutY="89.0" text="Interest Rate:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="interestRateLabel" layoutX="300.0" layoutY="89.0" prefHeight="25.0" prefWidth="111.0">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="goBackButton" layoutX="496.0" layoutY="31.0" mnemonicParsing="false" onAction="#goBack" prefHeight="33.0" prefWidth="68.0" text="Go Back" />
      <Button fx:id="langButton" layoutX="14.0" layoutY="35.0" mnemonicParsing="false" onAction="#toggleLang" text="FR" />
   </children>
</AnchorPane>
